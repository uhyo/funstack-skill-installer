{
  "name": "@funstack/skill-installer",
  "version": "1.0.0-alpha.4",
  "description": "CLI tool and library to install Agent Skills",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uhyo/funstack-skill-installer.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
