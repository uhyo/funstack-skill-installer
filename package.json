  "keywords": [ ],
  "author": "uhyo <uhyo@uhy.ooo>",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
